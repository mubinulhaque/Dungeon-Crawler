//W Key
if(keyboard_check(ord("W")) &amp;&amp; alarm[0] &lt;= 0) {
    if(!place_meeting(targetX + vectX, targetY + vectY, ObjWall)) {
        targetX += vectX
        targetY += vectY
        alarm[0] = room_speed / 12
    }
//S Key
} else if(keyboard_check(ord("S")) &amp;&amp; alarm[0] &lt;= 0) {
    if(!place_meeting(targetX - vectX, targetY - vectY, ObjWall)) {
        targetX -= vectX
        targetY -= vectY
        alarm[0] = room_speed / 12
    }
}
//D Key
if(keyboard_check(ord("D")) &amp;&amp; alarm[1] &lt;= 0) {
    targetDir -= 90
    alarm[1] = room_speed / 3
//A Key
} else if(keyboard_check(ord("A")) &amp;&amp; alarm[1] &lt;= 0) {
    targetDir += 90
    alarm[1] = room_speed / 3
}

//Escape Key
if(keyboard_check(vk_escape)) {
    game_end()
}
</string>
